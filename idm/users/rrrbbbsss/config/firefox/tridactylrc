" blank out
set nativeinstallcmd ""

" use emacs
set editorcmd emacsclient -c

" newtab page
set newtab about:blank

" search urls
set searchurls.gg https://google.com/search?q=
set searchurls.dg https://duckduckgo.com/?q=
set searchurls.wk https://en.wikipedia.org/wiki/
set searchurls.yt https://youtube.com/results?search_query=
set searchurls.no https://search.nixos.org/options?query=
set searchurls.np https://search.nixos.org/packages?query=
set searchurls.nh https://mipmip.github.io/home-manager-option-search/?query=
set searchurls.nf https://noogle.dev/?term=%22%s%22
" disable on sites
" blacklistadd ...
